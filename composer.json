{
  "name": "kotyaaa/keyvaluestore",
  "description": "Project for store pair key-value into array, JSON or YAML file formates",
  "keywords": ["project", "keyvaluestore"],
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alexandr Kotelevets",
      "email": "kotya.aa@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "symfony/yaml": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
